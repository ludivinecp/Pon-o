.navbar{
/* IE10+ */
background-image: -ms-linear-gradient(bottom, #0D0905 0%, #2E1706 100%);

/* Mozilla Firefox */
background-image: -moz-linear-gradient(bottom, #0D0905 0%, #2E1706 100%);

/* Opera */
background-image: -o-linear-gradient(bottom, #0D0905 0%, #2E1706 100%);

/* Webkit (Safari/Chrome 10) */
background-image: -webkit-gradient(linear, left bottom, left top, color-stop(0, #0D0905), color-stop(100, #2E1706));

/* Webkit (Chrome 11+) */
background-image: -webkit-linear-gradient(bottom, #0D0905 0%, #2E1706 100%);

/* W3C Markup */
background-image: linear-gradient(to top, #0D0905 0%, #2E1706 100%);
box-shadow: 0px 2px 2px rgba(0,0,0,0.37);

padding-right: 50px;
}


.block-left{
-webkit-box-shadow: 2px 0px 6px 0px rgba(0,0,0,0.37);
