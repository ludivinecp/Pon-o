<div class="input-group col-sm-12 search-bar ">
<%= form_tag services_path, method: 'get', class: 'navbar-form', role: 'search' do %>
  <div class="input-group">
  <%= text_field_tag :search, params[:search], size: 55, placeholder: "Rechercher un lieu", class: "form-control"%>
  </div>

  <%= button_tag(type: "submit", class: "btn btn-success btn-search") do %>
  <p >Hue!</p>
<% end %>
</div
<% end %>